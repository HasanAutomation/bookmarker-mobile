{
  "expo": {
    "name": "bookmarker-mobile",
    "slug": "bookmarker-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/bookmark.png",
    "splash": {
      "image": "./app/assets/splashNew.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.bookmarkerhasan.com",
      "versionCode": 1
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
